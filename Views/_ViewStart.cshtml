}*@
@{
    string layout = " ";
    if (ViewBag.Redirect != null)
    {
        layout = "~/Views/Shared/AfterLoginPage.cshtml";
    }
    else
    {
        layout = "~/Views/Shared/_Layout.cshtml";
    }
    Layout = layout;
}

